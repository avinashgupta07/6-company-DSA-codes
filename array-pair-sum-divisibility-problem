 bool canPair(vector<int> nums, int k) {
        // Code here.
        map<int,int> mp;
        for(int i=0;i<nums.size();i++){
            mp[nums[i]%k]++;
        }
        int r;
        for(int i=0;i<nums.size();i++){
            r=nums[i]%k;
            if(r==0){
                if(mp[r]&1)return 0;
            }
            else if(mp[r]!=mp[k-r])return 0;
        }
        return 1;
    }
